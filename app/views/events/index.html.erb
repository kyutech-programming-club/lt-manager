<h1>LT一覧</h1>
<%= link_to("新規作成", new_event_path) %>
<table class="table table-hover">
  <thead>
  <tr>
    <th>タイトル</th>
    <th>開催日</th>
  </tr>
  </thead>
  <tbody>
  <% @events.each do |event| %>
    <tr>
      <td>
        <%= link_to(event.title,  event_path(id: event.id)) %>
      </td>
      <td>
        <p><%= event.start_time %>~<%= event.end_time%></p>
      </td>
    </tr>
    </tbody>
    </table>

<% end %>
