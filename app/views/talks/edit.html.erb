<h1>トーク編集</h1>

<%= form_with model: @talk, url: talk_path, local: true do |f| %>
  <%= inform_validation_error @talk %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :slide_url%>
  <%= f.text_field :slide_url %>
  <%= f.label :movie_url %>
  <%= f.text_field :movie_url %>
  <%= f.submit %>
<% end %>
