<h1>陰キャ一覧</h1>
<table class="table">
  <thead>
  <tr>
    <th>ユーザー</th>
    <th>自己紹介</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%= link_to(user.name,  user_path(id: user.id)) %>
      </td>
      <td>
        <%= user.status %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
