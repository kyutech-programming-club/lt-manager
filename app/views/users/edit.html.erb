<h1>ユーザー編集</h1>
<%= form_with model: @user, local: true do |f| %>
  <%= inform_validation_error @user %>
  <%= f.label :name %>
  <%= f.text_field :name, class:"form-control" %>
  <%= f.label :status %>
  <%= f.text_field :status, class:"form-control" %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
